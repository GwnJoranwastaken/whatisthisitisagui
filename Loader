--Vars
local gameid = game.PlaceId
local jobid = game.JobId

--Settings
local Chat = _G.ShouldChat



function chat()
local A_1 = "Loading"
local A_2 = "All"
local Event = game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest
Event:FireServer(A_1, A_2)
wait(1)
local A_1 = "Gang Admin Loaded!"
local A_2 = "All"
local Event = game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest
Event:FireServer(A_1, A_2)
end
---------------------------------------------------------------------------------------------------------------------------------

if gameid == 6155643742 then
if Chat == "yes" then
local A_1 = "Ragdoll Engine Detected"
local A_2 = "All"
local Event = game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest
Event:FireServer(A_1, A_2)
wait(0.25)
chat()
loadstring(game:HttpGet("https://raw.githubusercontent.com/GwnJoranwastaken/whatisthisitisagui/main/FallBlock"))()
end

if Chat == "no" then
game.StarterGui:SetCore("SendNotification", {
Title = "Welcome",
Text = "Now Loading Please wait",
Duration = 3,
})
wait(3)
loadstring(game:HttpGet("https://raw.githubusercontent.com/GwnJoranwastaken/whatisthisitisagui/main/FallBlock"))()
game.StarterGui:SetCore("SendNotification", {
Title = "Loaded",
Text = "Have Fun!",
Duration = 3,
})
end
end
print(gameid)
if gameid == 2041312716 then
if Chat == "yes" then
game.StarterGui:SetCore("SendNotification", {
Title = "Welcome",
Text = "Now Loading Please wait",
Duration = 3,
})
local A_1 = "Ragdoll Engine Detected"
local A_2 = "All"
local Event = game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest
Event:FireServer(A_1, A_2)
chat()

end

if Chat == "no" then
game.StarterGui:SetCore("SendNotification", {
Title = "Welcome",
Text = "Now Loading Please wait",
Duration = 3,
})
wait(3)
loadstring(game:HttpGet("https://raw.githubusercontent.com/GwnJoranwastaken/whatisthisitisagui/main/FallBlock"))()
game.StarterGui:SetCore("SendNotification", {
Title = "Loaded",
Text = "Have Fun!",
Duration = 3,
})
end
end
